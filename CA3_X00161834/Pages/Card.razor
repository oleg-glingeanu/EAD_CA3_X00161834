<MudCard>
    <MudCardHeader>
        @Name
    </MudCardHeader>
    <MudCardMedia Image="@Image" />
    <MudCardContent>
        @Price
    </MudCardContent>
</MudCard>


@code {
    [Parameter]
    public string Image { get; set; } = null!;

    [Parameter]
    public RenderFragment? Name { get; set; } 

    [Parameter]
    public RenderFragment Price { get; set; } = null!;
}